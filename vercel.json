{
    "routes": [
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/dashboard",
        "dest": "/dashboard.html"
      },
      {
        "src": "/css/(.*)",
        "dest": "/public/css/$1"
      },
      {
        "src": "/js/(.*)",
        "dest": "/public/js/$1"
      },
      {
        "src": "/img/(.*)",
        "dest": "/public/img/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "status": 404
      }
    ]
  }