{
    "version": 2,
    "builds": [
      {
        "src": "public/css/output.css",
        "use": "@vercel/static"
      },
      {
        "src": "public/js/*.js",
        "use": "@vercel/static"
      },
      {
        "src": "src/*.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/index.html"
      }
    ]
  }
  