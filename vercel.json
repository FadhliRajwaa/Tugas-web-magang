{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "command": "npm run dev"
        }
      },
      {
        "src": "src/*.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/dashboard",
        "dest": "/src/dashboard.html"
      },
      {
        "src": "/(.*)",
        "dest": "/src/index.html"
      }
    ]
  }