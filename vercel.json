{
    "routes": [
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/dashboard",
        "dest": "/dashboard.html"
      },
      {
        "src": "/css/(.*)",
        "dest": "/public/css/$1"
      },
      {
        "src": "/js/(.*)",
        "dest": "/public/js/$1"
      },
      {
        "src": "/img/(.*)",
        "dest": "/public/img/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "status": 404
      }
    ],
    "headers": [
      {
        "source": "/public/css/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/public/js/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/public/img/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=604800, must-revalidate"
          }
        ]
      }
    ]
  }